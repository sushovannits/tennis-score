{
  "name": "graphql-tennis-scoring-service",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "start": "nodemon src/index.ts",
    "test": "yarn run lint && jest"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.10",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@types/expect": "^1.20.4",
    "@types/jest": "^24.0.12",
    "apollo-server": "^2.9.4",
    "eslint": "^7.11.0",
    "jest": "^24.8.0",
    "expect": "^24.8.0",
    "graphql": "^14.5.8",
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "ts-jest": "^24.0.2",
    "prettier": "^1.19.1",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "node-fetch": "^2.6.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }

}
